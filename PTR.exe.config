<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="ProcessWideConfig" type="PTR.ProcessWideConfigSection, PTR"/>
		<section name="TestingConfigurations" type="PTR.TestingConfigurationsSection, PTR"/>
		<section name="SharedResources" type="PTR.SharedResourcesSection, PTR"/>
	</configSections>

	<ProcessWideConfig 	LoggingLocation=""
						MaxThreads="6*"
						BeforeTestExecution=""
						AfterTestExecution=""
						SemicolonSeparatedFilesHavingTestCases="*"
						TestRunner="Nunit3TestRunner.bat*"
						ExecutionLocation=""
						TestingProjectLocation="*"
						TestCasesExtractor="DotNetTestCasesExtractor.exe*"
						TestCategories=""
						TestClasses=""
						SemicolonSeparatedTestCases=""
						SemicolonSeparatedTestCasesToBeSkipped=""
						BeforeRunConfigEditor="XmlEditor.exe @Param/Path:XPaths.txt @Param/Path:ConfigFile.xml @Param/SharedResources:SR_1;SR_2"
						TimesToRerunFailedTestCases="0"
						BeforeRerunConfigEditor=""
						ThreadCount="1"
						MinBucketSize="3"
						MaxBucketSize="20"
						ConcurrentUnit="2"
						ReportProcessor="NUnitReportUtility.exe*" />

	<TestingConfigurations>
		<Config	ID="UnitTestCases*"
				IsEnabled="true"
				SemicolonSeparatedConfigResultsToBeMergedInto=""
				SemicolonSeparatedFilesHavingTestCases="*"
				TestRunner="Nunit3TestRunner.bat*"
				ExecutionLocation=""
				ReportingLocation="C:\TestExecutionLocation\TestResults.xml"
				TestingProjectLocation="*"
				TestCasesExtractor="DotNetTestCasesExtractor.exe*"
				TestCategories=""
				TestClasses=""
				SemicolonSeparatedTestCases=""
				SemicolonSeparatedTestCasesToBeSkipped=""
				BeforeRunConfigEditor="XmlEditor.exe @Param/Path:XPaths.txt @Param/Path:PTR.exe.config @Param/SharedResources:SR_1;SR_2"
				TimesToRerunFailedTestCases="0"
				BeforeRerunConfigEditor=""
				ThreadCount="1"
				MinBucketSize="3"
				MaxBucketSize="20"
				ConcurrentUnit="2"
				ReportProcessor="NUnitReportUtility.exe*"
				CleanAfterCompletion="true" />
	</TestingConfigurations>

	<SharedResources>
		<add ID="SR_1" SemicolonSeparatedResources="VAL:=:2"/>
		<add ID="SR_2" SemicolonSeparatedResources="VAL:=:3"/>
	</SharedResources>
</configuration>